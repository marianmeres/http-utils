		"url": "https://github.com/marianmeres/http-utils/issues"
	},
	"homepage": "https://github.com/marianmeres/http-utils#readme",
	"devDependencies": {
		"@marianmeres/release": "^1.1.2",
		"@marianmeres/test-runner": "^2.0.16",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/node": "^22.3.0",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"rollup": "^4.20.0",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4"
	},
