{
	"name": "@marianmeres/http-utils",
	"version": "1.9.0",
	"description": "Misc DRY http fetch related helpers",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
